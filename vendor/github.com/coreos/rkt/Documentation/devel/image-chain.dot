digraph G {
    rankdir="LR";

    graph [fontname = "helvetica"];
    node [fontname = "Arial", fillcolor="#FFE599", style="filled"];
    edge [fontname = "monospace"];
  
    {
        fetch [shape="node", label=<Fetch>]
        store [shape="node", label=<Store>]
        render [shape="node", label=<Render>]
    }

    fetch -> store
    store -> render
}
